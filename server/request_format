    "type":GET_FILE,
    "data":{
        "filename":
        "data":
    }
}


sendPrivateFile
{
    "type":SEND_PRIVATE_FILE,
    "token":
    "data":{
        "user_id":
        "time":
        "to_id":
        "filename":
        "data":
    }
}
{
    "type":SNED_PRIVATE_FILE,
    "data":{
        "result":"send file successfully"
    }
}


